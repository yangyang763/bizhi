<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.android32_asynctask_json">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SET_WALLPAPER" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name="com.bane.mymain.MyMainActivity" />
        <activity android:name="com.bane.flash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.bane.mymain.FirstActivity" />
        <activity android:name="com.bane.mymain.ClickActivity" />
        <activity android:name="com.bane.mymain.CategoryActivity" />
        <activity android:name="com.bane.mymain.SearchActivity"></activity>
        <activity android:name="com.bane.mymain.ViewPagerActivity"></activity>
    </application>

</manifest>